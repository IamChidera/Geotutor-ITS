<?xml version="1.0"?>
<rdf:RDF xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
         xmlns:xsd="http://www.w3.org/2001/XMLSchema#"
         xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#"
         xmlns:owl="http://www.w3.org/2002/07/owl#"
         xml:base="http://geotutor.yorksj.ac.uk/onto"
         xmlns="http://geotutor.yorksj.ac.uk/onto#"
         xmlns:swrl="http://www.w3.org/2003/11/swrl#">

<owl:Ontology rdf:about="http://geotutor.yorksj.ac.uk/onto"/>

<owl:ObjectProperty rdf:about="#studies">
  <rdfs:domain rdf:resource="#Student"/>
  <rdfs:range rdf:resource="#Resource"/>
</owl:ObjectProperty>

<owl:ObjectProperty rdf:about="#requiresPrerequisite">
  <rdfs:domain rdf:resource="#Resource"/>
  <rdfs:range rdf:resource="#Resource"/>
</owl:ObjectProperty>

<owl:DatatypeProperty rdf:about="#hasBase">
  <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
  <rdfs:domain rdf:resource="#Triangle"/>
  <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#decimal"/>
</owl:DatatypeProperty>

<owl:DatatypeProperty rdf:about="#hasHeight">
  <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
  <rdfs:domain rdf:resource="#Triangle"/>
  <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#decimal"/>
</owl:DatatypeProperty>

<owl:DatatypeProperty rdf:about="#hasSide">
  <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
  <rdfs:domain rdf:resource="#Square"/>
  <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#decimal"/>
</owl:DatatypeProperty>

<owl:DatatypeProperty rdf:about="#hasLength">
  <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
  <rdfs:domain rdf:resource="#Rectangle"/>
  <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#decimal"/>
</owl:DatatypeProperty>

<owl:DatatypeProperty rdf:about="#hasWidth">
  <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
  <rdfs:domain rdf:resource="#Rectangle"/>
  <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#decimal"/>
</owl:DatatypeProperty>

<owl:DatatypeProperty rdf:about="#hasArea">
  <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
  <rdfs:domain rdf:resource="#Shape"/>
  <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#decimal"/>
</owl:DatatypeProperty>

<owl:DatatypeProperty rdf:about="#explains">
  <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
  <rdfs:domain rdf:resource="#Shape"/>
  <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
</owl:DatatypeProperty>

<owl:DatatypeProperty rdf:about="#hasMastery">
  <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
  <rdfs:domain rdf:resource="#Student"/>
  <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#decimal"/>
</owl:DatatypeProperty>

<owl:DatatypeProperty rdf:about="#hasDifficultyLevel">
  <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
  <rdfs:domain rdf:resource="#Resource"/>
  <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
</owl:DatatypeProperty>

<owl:Class rdf:about="#Shape">
  <rdfs:subClassOf rdf:resource="http://www.w3.org/2002/07/owl#Thing"/>
</owl:Class>

<owl:Class rdf:about="#Triangle">
  <rdfs:subClassOf rdf:resource="#Shape"/>
  <rdfs:subClassOf rdf:resource="#Resource"/>
</owl:Class>

<owl:Class rdf:about="#Square">
  <rdfs:subClassOf rdf:resource="#Shape"/>
  <rdfs:subClassOf rdf:resource="#Resource"/>
</owl:Class>

<owl:Class rdf:about="#Rectangle">
  <rdfs:subClassOf rdf:resource="#Shape"/>
  <rdfs:subClassOf rdf:resource="#Resource"/>
</owl:Class>

<owl:Class rdf:about="#Student">
  <rdfs:subClassOf rdf:resource="http://www.w3.org/2002/07/owl#Thing"/>
</owl:Class>

<owl:Class rdf:about="#Resource">
  <rdfs:subClassOf rdf:resource="http://www.w3.org/2002/07/owl#Thing"/>
</owl:Class>

<owl:NamedIndividual rdf:about="#DemoStudent">
  <rdf:type rdf:resource="#Student"/>
  <studies rdf:resource="#Triangle_Easy"/>
  <studies rdf:resource="#Square_Medium"/>
  <hasMastery rdf:datatype="http://www.w3.org/2001/XMLSchema#decimal">0.65</hasMastery>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#Triangle_Easy">
  <rdf:type rdf:resource="#Resource"/>
  <hasDifficultyLevel rdf:datatype="http://www.w3.org/2001/XMLSchema#string">easy</hasDifficultyLevel>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#Square_Medium">
  <rdf:type rdf:resource="#Resource"/>
  <hasDifficultyLevel rdf:datatype="http://www.w3.org/2001/XMLSchema#string">medium</hasDifficultyLevel>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#DemoTriangle">
  <rdf:type rdf:resource="#Triangle"/>
  <hasBase rdf:datatype="http://www.w3.org/2001/XMLSchema#decimal">10.0</hasBase>
  <hasHeight rdf:datatype="http://www.w3.org/2001/XMLSchema#decimal">6.0</hasHeight>
</owl:NamedIndividual>

<swrl:Variable rdf:about="urn:swrl#t"/>

<swrl:Variable rdf:about="urn:swrl#b"/>

<swrl:Variable rdf:about="urn:swrl#h"/>

<swrl:Variable rdf:about="urn:swrl#temp"/>

<swrl:Variable rdf:about="urn:swrl#a"/>

<swrl:Variable rdf:about="urn:swrl#s"/>

<swrl:Variable rdf:about="urn:swrl#side"/>

<swrl:Variable rdf:about="urn:swrl#r"/>

<swrl:Variable rdf:about="urn:swrl#l"/>

<swrl:Variable rdf:about="urn:swrl#w"/>

<swrl:Imp>
  <swrl:body>
    <rdf:Description>
      <rdf:first>
        <swrl:ClassAtom>
          <swrl:classPredicate rdf:resource="#Rectangle"/>
          <swrl:argument1 rdf:resource="urn:swrl#r"/>
        </swrl:ClassAtom>
      </rdf:first>
      <rdf:rest>
        <rdf:Description>
          <rdf:first>
            <swrl:DatavaluedPropertyAtom>
              <swrl:propertyPredicate rdf:resource="#hasLength"/>
              <swrl:argument1 rdf:resource="urn:swrl#r"/>
              <swrl:argument2 rdf:resource="urn:swrl#l"/>
            </swrl:DatavaluedPropertyAtom>
          </rdf:first>
          <rdf:rest>
            <rdf:Description>
              <rdf:first>
                <swrl:DatavaluedPropertyAtom>
                  <swrl:propertyPredicate rdf:resource="#hasWidth"/>
                  <swrl:argument1 rdf:resource="urn:swrl#r"/>
                  <swrl:argument2 rdf:resource="urn:swrl#w"/>
                </swrl:DatavaluedPropertyAtom>
              </rdf:first>
              <rdf:rest>
                <rdf:Description>
                  <rdf:first>
                    <swrl:BuiltinAtom>
                      <swrl:builtin rdf:resource="http://www.w3.org/2003/11/swrlb#multiply"/>
                      <swrl:arguments>
                        <rdf:Description>
                          <rdf:first rdf:resource="urn:swrl#a"/>
                          <rdf:rest>
                            <rdf:Description>
                              <rdf:first rdf:resource="urn:swrl#l"/>
                              <rdf:rest>
                                <rdf:Description>
                                  <rdf:first rdf:resource="urn:swrl#w"/>
                                  <rdf:rest rdf:resource="http://www.w3.org/1999/02/22-rdf-syntax-ns#nil"/>
                                </rdf:Description>
                              </rdf:rest>
                            </rdf:Description>
                          </rdf:rest>
                        </rdf:Description>
                      </swrl:arguments>
                    </swrl:BuiltinAtom>
                  </rdf:first>
                  <rdf:rest rdf:resource="http://www.w3.org/1999/02/22-rdf-syntax-ns#nil"/>
                </rdf:Description>
              </rdf:rest>
            </rdf:Description>
          </rdf:rest>
        </rdf:Description>
      </rdf:rest>
    </rdf:Description>
  </swrl:body>
  <swrl:head>
    <rdf:Description>
      <rdf:first>
        <swrl:DatavaluedPropertyAtom>
          <swrl:propertyPredicate rdf:resource="#hasArea"/>
          <swrl:argument1 rdf:resource="urn:swrl#r"/>
          <swrl:argument2 rdf:resource="urn:swrl#a"/>
        </swrl:DatavaluedPropertyAtom>
      </rdf:first>
      <rdf:rest>
        <rdf:Description>
          <rdf:first>
            <swrl:DatavaluedPropertyAtom>
              <swrl:propertyPredicate rdf:resource="#explains"/>
              <swrl:argument1 rdf:resource="urn:swrl#r"/>
              <swrl:argument2 rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Area = length × width</swrl:argument2>
            </swrl:DatavaluedPropertyAtom>
          </rdf:first>
          <rdf:rest rdf:resource="http://www.w3.org/1999/02/22-rdf-syntax-ns#nil"/>
        </rdf:Description>
      </rdf:rest>
    </rdf:Description>
  </swrl:head>
</swrl:Imp>

<swrl:Imp>
  <swrl:body>
    <rdf:Description>
      <rdf:first>
        <swrl:ClassAtom>
          <swrl:classPredicate rdf:resource="#Square"/>
          <swrl:argument1 rdf:resource="urn:swrl#s"/>
        </swrl:ClassAtom>
      </rdf:first>
      <rdf:rest>
        <rdf:Description>
          <rdf:first>
            <swrl:DatavaluedPropertyAtom>
              <swrl:propertyPredicate rdf:resource="#hasSide"/>
              <swrl:argument1 rdf:resource="urn:swrl#s"/>
              <swrl:argument2 rdf:resource="urn:swrl#side"/>
            </swrl:DatavaluedPropertyAtom>
          </rdf:first>
          <rdf:rest>
            <rdf:Description>
              <rdf:first>
                <swrl:BuiltinAtom>
                  <swrl:builtin rdf:resource="http://www.w3.org/2003/11/swrlb#multiply"/>
                  <swrl:arguments>
                    <rdf:Description>
                      <rdf:first rdf:resource="urn:swrl#a"/>
                      <rdf:rest>
                        <rdf:Description>
                          <rdf:first rdf:resource="urn:swrl#side"/>
                          <rdf:rest>
                            <rdf:Description>
                              <rdf:first rdf:resource="urn:swrl#side"/>
                              <rdf:rest rdf:resource="http://www.w3.org/1999/02/22-rdf-syntax-ns#nil"/>
                            </rdf:Description>
                          </rdf:rest>
                        </rdf:Description>
                      </rdf:rest>
                    </rdf:Description>
                  </swrl:arguments>
                </swrl:BuiltinAtom>
              </rdf:first>
              <rdf:rest rdf:resource="http://www.w3.org/1999/02/22-rdf-syntax-ns#nil"/>
            </rdf:Description>
          </rdf:rest>
        </rdf:Description>
      </rdf:rest>
    </rdf:Description>
  </swrl:body>
  <swrl:head>
    <rdf:Description>
      <rdf:first>
        <swrl:DatavaluedPropertyAtom>
          <swrl:propertyPredicate rdf:resource="#hasArea"/>
          <swrl:argument1 rdf:resource="urn:swrl#s"/>
          <swrl:argument2 rdf:resource="urn:swrl#a"/>
        </swrl:DatavaluedPropertyAtom>
      </rdf:first>
      <rdf:rest>
        <rdf:Description>
          <rdf:first>
            <swrl:DatavaluedPropertyAtom>
              <swrl:propertyPredicate rdf:resource="#explains"/>
              <swrl:argument1 rdf:resource="urn:swrl#s"/>
              <swrl:argument2 rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Area = side²</swrl:argument2>
            </swrl:DatavaluedPropertyAtom>
          </rdf:first>
          <rdf:rest rdf:resource="http://www.w3.org/1999/02/22-rdf-syntax-ns#nil"/>
        </rdf:Description>
      </rdf:rest>
    </rdf:Description>
  </swrl:head>
</swrl:Imp>

<swrl:Imp>
  <swrl:body>
    <rdf:Description>
      <rdf:first>
        <swrl:ClassAtom>
          <swrl:classPredicate rdf:resource="#Triangle"/>
          <swrl:argument1 rdf:resource="urn:swrl#t"/>
        </swrl:ClassAtom>
      </rdf:first>
      <rdf:rest>
        <rdf:Description>
          <rdf:first>
            <swrl:DatavaluedPropertyAtom>
              <swrl:propertyPredicate rdf:resource="#hasBase"/>
              <swrl:argument1 rdf:resource="urn:swrl#t"/>
              <swrl:argument2 rdf:resource="urn:swrl#b"/>
            </swrl:DatavaluedPropertyAtom>
          </rdf:first>
          <rdf:rest>
            <rdf:Description>
              <rdf:first>
                <swrl:DatavaluedPropertyAtom>
                  <swrl:propertyPredicate rdf:resource="#hasHeight"/>
                  <swrl:argument1 rdf:resource="urn:swrl#t"/>
                  <swrl:argument2 rdf:resource="urn:swrl#h"/>
                </swrl:DatavaluedPropertyAtom>
              </rdf:first>
              <rdf:rest>
                <rdf:Description>
                  <rdf:first>
                    <swrl:BuiltinAtom>
                      <swrl:builtin rdf:resource="http://www.w3.org/2003/11/swrlb#multiply"/>
                      <swrl:arguments>
                        <rdf:Description>
                          <rdf:first rdf:resource="urn:swrl#temp"/>
                          <rdf:rest>
                            <rdf:Description>
                              <rdf:first rdf:resource="urn:swrl#b"/>
                              <rdf:rest>
                                <rdf:Description>
                                  <rdf:first rdf:resource="urn:swrl#h"/>
                                  <rdf:rest rdf:resource="http://www.w3.org/1999/02/22-rdf-syntax-ns#nil"/>
                                </rdf:Description>
                              </rdf:rest>
                            </rdf:Description>
                          </rdf:rest>
                        </rdf:Description>
                      </swrl:arguments>
                    </swrl:BuiltinAtom>
                  </rdf:first>
                  <rdf:rest>
                    <rdf:Description>
                      <rdf:first>
                        <swrl:BuiltinAtom>
                          <swrl:builtin rdf:resource="http://www.w3.org/2003/11/swrlb#divide"/>
                          <swrl:arguments>
                            <rdf:Description>
                              <rdf:first rdf:resource="urn:swrl#a"/>
                              <rdf:rest>
                                <rdf:Description>
                                  <rdf:first rdf:resource="urn:swrl#temp"/>
                                  <rdf:rest>
                                    <rdf:Description>
                                      <rdf:rest rdf:resource="http://www.w3.org/1999/02/22-rdf-syntax-ns#nil"/>
                                      <rdf:first rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">2</rdf:first>
                                    </rdf:Description>
                                  </rdf:rest>
                                </rdf:Description>
                              </rdf:rest>
                            </rdf:Description>
                          </swrl:arguments>
                        </swrl:BuiltinAtom>
                      </rdf:first>
                      <rdf:rest rdf:resource="http://www.w3.org/1999/02/22-rdf-syntax-ns#nil"/>
                    </rdf:Description>
                  </rdf:rest>
                </rdf:Description>
              </rdf:rest>
            </rdf:Description>
          </rdf:rest>
        </rdf:Description>
      </rdf:rest>
    </rdf:Description>
  </swrl:body>
  <swrl:head>
    <rdf:Description>
      <rdf:first>
        <swrl:DatavaluedPropertyAtom>
          <swrl:propertyPredicate rdf:resource="#hasArea"/>
          <swrl:argument1 rdf:resource="urn:swrl#t"/>
          <swrl:argument2 rdf:resource="urn:swrl#a"/>
        </swrl:DatavaluedPropertyAtom>
      </rdf:first>
      <rdf:rest>
        <rdf:Description>
          <rdf:first>
            <swrl:DatavaluedPropertyAtom>
              <swrl:propertyPredicate rdf:resource="#explains"/>
              <swrl:argument1 rdf:resource="urn:swrl#t"/>
              <swrl:argument2 rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Area = ½ × base × height</swrl:argument2>
            </swrl:DatavaluedPropertyAtom>
          </rdf:first>
          <rdf:rest rdf:resource="http://www.w3.org/1999/02/22-rdf-syntax-ns#nil"/>
        </rdf:Description>
      </rdf:rest>
    </rdf:Description>
  </swrl:head>
</swrl:Imp>

<owl:AllDisjointClasses>
  <owl:members rdf:parseType="Collection">
    <rdf:Description rdf:about="#Triangle"/>
    <rdf:Description rdf:about="#Square"/>
    <rdf:Description rdf:about="#Rectangle"/>
  </owl:members>
</owl:AllDisjointClasses>


